<script>


  jQuery(document).ready(function($) {

        $('.demo01, .demo02, .demo03, .demo04').lazyTube({
            targetHandlers: {
                magnificPopup: function(options, params) {
                    $.magnificPopup.open({
                        items: [{
                            src: 'https://www.youtube.com/watch/?v=' + params.id + '?showinfo=0&;rel=0&autoplay=1;',
                            type: 'iframe'
                        }]
                    });
                }
            }
        });
    });

</script>
