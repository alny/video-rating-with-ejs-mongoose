<% layout('layouts/layout') -%>


<div class="container-fluid half-padding">
  <div class="pages pages_dashboard">
    <div class="row">

<% for(var i = 0; i < foundAll.length; i++) { %>

      <div class="col-md-2">
        <div class="panel panel-warning">
          <div class="panel-heading">
            <li class="dropdown" style="margin-bottom: -20px;">
              <h3 style="overflow: hidden;text-overflow: ellipsis;white-space: nowrap"class="panel-title"><%= foundAll[i].title %></h3>
              <a href="#" data-toggle="dropdown" class="dropdown-toggle"><i class="caret" style="position: relative;right: -223px;top: -22px;color:antiquewhite;"></i></a>
              <ul class="dropdown-menu" style="top: 48%;left: 79px;!important">


                    <li><a href="#"><span>By <%= foundUser.ownByUser.profile.name %> </span></a></li>



                <% if(user) { %>
                <li role="separator" class="divider"></li>
                <li><a href="/report/<%= foundAll[i].videoLink %>">Report</a></li>
                <% } %>

              </ul>
            </li>
          </div>
          <span data-id="<%= foundAll[i].videoLink %>" data-thumbnail="maxresdefault" data-target="magnificPopup" class="demo02"></span>

<div class="wrapper-rating">
          <div class="sp-widget__info" style="margin-bottom:10px;">
          <div class="sp-widget__title"><i style="margin-left:15px;color:#eb3232;margin-right:10px;" class="fa fa-crosshairs"></i><span class="frag-style"><%= foundAll[i].category %></span></div>
          </div>

          <a href="" name="" type="button" style="margin-bottom:8px;padding: 2px 6px 2px 6px;" class="btn btn-warning"><i class="fa fa-television" style="font-size:14px;" aria-hidden="true"></i></a>

          <a href="" name="" type="button" style="margin-bottom:8px;padding: 2px 6px 2px 6px;" class="btn btn-success"><i class="fa fa-tachometer" style="font-size:14px;" aria-hidden="true"></i></a>


          <a href="" name="" type="button" style="margin-bottom:8px;padding: 2px 6px 2px 6px;" class="btn btn-primary"><i class="fa fa-users" style="font-size:14px;" aria-hidden="true"></i></a>

</div>


        </div>
      </div>
<% } %>


    </div>
  </div>
</div>
